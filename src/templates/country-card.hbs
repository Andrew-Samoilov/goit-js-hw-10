<div class="card">
  <div class="card-img-top">
    <img class="card-flag" src="{{[0].flags.svg}}" alt="Flag {{[0].name.official}}">
    <h2 class="card-title">{{[0].name.official}}</h2>
  </div>
  <div class="card-body">
    <p><b>Capital:</b> {{[0].capital}}</p>
    <p><b>Population:</b> {{[0].population}}</p>
    <p><b>Languages:</b> {{...[0].languages}}</p>
    <p><b>Languages</b></p>
    <ul class="list-group"></ul>
    {{#each [0].languages}}
      <li class="list-group-item">{{[0].languages.[...]}}</li>
    {{/each}}
    </ul>
  </div>
</div>
{{!-- name,capital,population,flags,languages --}}